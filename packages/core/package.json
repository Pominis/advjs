    "dev": "tsdown --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@advjs/parser": "workspace:*",
    "@advjs/types": "workspace:*",
    "@types/mdast": "catalog:",
    "@vueuse/core": "catalog:",
    "consola": "catalog:",
    "html2canvas": "catalog:",
    "pixi.js": "catalog:",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "vue": "catalog:"
