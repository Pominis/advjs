{
  "name": "@advjs/parser",
  "type": "module",
  "version": "0.0.1",
  "description": "A parser for advjs.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "homepage": "https://parser.advjs.org",
  "keywords": [
    "advjs",
    "parser",
    "ast"
  ],
  "readme": "https://github.com/YunYouJun/advjs/tree/main/packages/parser#readme",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./fs": {
      "types": "./fs.d.ts",
      "default": "./dist/fs.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": "^20.0.0 || >=22.0.0"
  },
  "scripts": {
    "postinstall": "npm run copy",
    "build": "tsdown",
    "copy": "tsx scripts/copy.ts",
    "dev": "tsdown --watch",
    "play": "npm -C playground run dev",
    "play:build": "npm run copy && npm -C playground run build",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "consola": "*"
  },
  "dependencies": {
    "@advjs/types": "workspace:*",
    "@advjs/unocss": "workspace:*",
    "@types/mdast": "catalog:",
    "js-yaml": "catalog:",
    "mdast-util-to-hast": "catalog:",
    "rehype-stringify": "catalog:",
    "remark": "catalog:",
    "remark-frontmatter": "catalog:",
    "remark-gfm": "catalog:",
    "remark-parse": "catalog:",
    "remark-rehype": "catalog:",
    "to-vfile": "catalog:",
    "unified": "catalog:",
    "vfile-matter": "catalog:"
  },
  "devDependencies": {
    "@types/js-yaml": "catalog:",
    "tsdown": "catalog:"
  }
}
