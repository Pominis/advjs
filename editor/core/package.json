    "@iconify-json/twemoji": "catalog:",
    "@nuxt/devtools": "catalog:",
    "@nuxtjs/color-mode": "catalog:",
    "@pinia/nuxt": "catalog:",
    "@unocss/eslint-config": "catalog:",
    "@unocss/nuxt": "catalog:",
    "@vite-pwa/nuxt": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "baklavajs": "catalog:",
    "consola": "catalog:",
    "honeycomb-grid": "catalog:",
    "nuxt": "catalog:",
    "nuxt-monaco-editor": "git+https://github.com/jontybrook/nuxt-monaco-editor.git#patch1",
    "pinia": "catalog:"
  }
