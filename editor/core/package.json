{
  "name": "@advjs/editor",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "description": "ADV.JS Editor",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
