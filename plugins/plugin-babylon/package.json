{
  "name": "@advjs/plugin-babylon",
  "type": "module",
  "version": "0.1.0",
  "description": "Babylon Plugin for ADV.JS to support 3D",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/YunYouJun/advjs/tree/main/packages/plugin-babylon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunYouJun/advjs.git"
  },
  "bugs": {
    "url": "https://github.com/YunYouJun/advjs/issues"
  },
  "keywords": [
    "advjs",
    "babylon",
    "vrm"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@advjs/types": "workspace:*",
    "@babylonjs/core": "catalog:",
    "@babylonjs/gui": "catalog:",
    "@babylonjs/loaders": "catalog:",
    "@babylonjs/materials": "catalog:",
    "babylon-vrm-loader": "catalog:"
  },
  "devDependencies": {
    "unbuild": "catalog:"
  }
}
